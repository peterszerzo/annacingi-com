{
  "name": "annacingi.com",
  "description": "Source code for annacingi.com",
  "version": "0.1.0",
  "scripts": {
    "start": "sapper dev",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy",
    "deploy": "npm run export && aws s3 sync __sapper__/export s3://annacingi.com --acl public-read",
    "cacheInvalidate": "aws cloudfront create-invalidation --distribution-id $CLOUDFRONT_DISTRIBUTION_ID --paths '/'"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "date-fns": "^2.20.0",
    "front-matter": "^3.1.0",
    "glob": "^7.1.6",
    "marked": "^4.0.10",
    "mz": "^2.7.0",
    "polka": "next",
    "ramda": "^0.26.1",
    "siema": "^1.5.1",
    "sirv": "^0.4.0",
    "snarkdown": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.12.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "sapper": "^0.27.12",
    "svelte": "^3.20.1"
  }
}
